<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/menu_constraint_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:background="@color/background"
        app:layout_constraintBottom_toTopOf="@+id/netie_window"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="@dimen/_19sdp"
            android:layout_marginTop="@dimen/_19sdp"
            android:layout_marginBottom="@dimen/_6sdp"
            android:padding="@dimen/_4sdp"
            android:text="@string/title_weather_forecast"
            android:textSize="@dimen/_22ssp"
            android:textColor="@color/text"
            android:background="@drawable/rounded_button_clickable"
            android:maxLines="1"
            android:ellipsize="end"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/menu_constraint_layout2">

        </androidx.appcompat.widget.AppCompatTextView>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menu_constraint_layout2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginHorizontal="@dimen/_19sdp"
            android:layout_marginVertical="@dimen/_6sdp"
            app:layout_constraintBottom_toTopOf="@+id/menu_constraint_layout3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/title">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/day1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_6sdp"
                android:background="@drawable/rounded_button_clickable"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/day2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:focusable="true">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_weekday1"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/icon_day1">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/icon_day1"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/text_weekday1"
                    app:layout_constraintEnd_toEndOf="@+id/text_day1"
                    app:layout_constraintBottom_toTopOf="@+id/text_day1">

                </androidx.appcompat.widget.AppCompatImageView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_day1"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textStyle="bold"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toBottomOf="@+id/icon_day1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                </androidx.appcompat.widget.AppCompatTextView>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/day2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_6sdp"
                android:background="@drawable/rounded_button_clickable"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/day1"
                android:focusable="true">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_weekday2"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/icon_day2">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/icon_day2"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/text_weekday2"
                    app:layout_constraintEnd_toEndOf="@+id/text_day2"
                    app:layout_constraintBottom_toTopOf="@+id/text_day2">

                </androidx.appcompat.widget.AppCompatImageView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_day2"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textStyle="bold"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toBottomOf="@+id/icon_day2"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                </androidx.appcompat.widget.AppCompatTextView>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/menu_constraint_layout3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginHorizontal="@dimen/_19sdp"
            android:layout_marginTop="@dimen/_6sdp"
            android:layout_marginBottom="@dimen/_19sdp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/menu_constraint_layout2">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/day3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginEnd="@dimen/_6sdp"
                android:background="@drawable/rounded_button_clickable"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/day4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:focusable="true">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_weekday3"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/icon_day3">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/icon_day3"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/text_weekday3"
                    app:layout_constraintEnd_toEndOf="@+id/text_day3"
                    app:layout_constraintBottom_toTopOf="@+id/text_day3">

                </androidx.appcompat.widget.AppCompatImageView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_day3"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textStyle="bold"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toBottomOf="@+id/icon_day3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                </androidx.appcompat.widget.AppCompatTextView>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/day4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_6sdp"
                android:background="@drawable/rounded_button_clickable"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/day3"
                android:focusable="true">

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_weekday4"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toTopOf="@+id/icon_day4">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/icon_day4"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="H,1:1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/text_weekday4"
                    app:layout_constraintEnd_toEndOf="@+id/text_day4"
                    app:layout_constraintBottom_toTopOf="@+id/text_day4">

                </androidx.appcompat.widget.AppCompatImageView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/text_day4"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_32sdp"
                    android:layout_margin="@dimen/_6sdp"
                    android:textSize="@dimen/_19ssp"
                    android:textStyle="bold"
                    android:textColor="@color/text"
                    android:gravity="center"
                    android:maxLines="1"
                    android:ellipsize="end"
                    app:layout_constraintTop_toBottomOf="@+id/icon_day4"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent">

                </androidx.appcompat.widget.AppCompatTextView>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <include
        layout="@layout/netie_window"
        android:id="@+id/netie_window">
    </include>

</androidx.constraintlayout.widget.ConstraintLayout>